<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".activity.login_register.RegisterProtocolFragment">

    <!-- TODO: Update blank fragment layout -->
    <include layout="@layout/common_toolbar"></include>

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line"></View>

    <com.likeits.simple.view.BorderTextView
        android:id="@+id/tv_confirm"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dip_xxlarge"
        android:layout_marginBottom="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dip_xxlarge"
        android:layout_marginRight="@dimen/dip_xxlarge"
        android:layout_marginTop="@dimen/dp_10"
        app:contentBackColor="#999999"
        app:strokeColor01="#999999"
        app:strokeWidth01="1dp"
        app:cornerRadius="@dimen/dp_5"
        android:gravity="center"
        android:padding="8dip"
        android:text="@string/login_confirm"
        android:textColor="@color/white" />

</LinearLayout>
